	        Light = false
	        Dark = true
	        script.Parent.Parent.BackgroundColor3 = Color3.new(0.231373, 0.231373, 0.231373)
	        script.Parent.Parent.Parent.Credits.BackgroundColor3 = Color3.new(0.231373, 0.231373, 0.231373)
